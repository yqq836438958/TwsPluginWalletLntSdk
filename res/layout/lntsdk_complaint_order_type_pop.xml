<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#292C38"
    android:orientation="vertical"
    android:padding="10dp" >
    <TextView 
        android:id="@+id/lntsdk_order_type_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="订单类型"
        android:textColor="@color/white"
        android:textSize="22sp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        />
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="1dp"
		android:background="@color/white"  
		android:layout_marginLeft="10dp"
		android:layout_marginRight="10dp"
        ></LinearLayout>
    <LinearLayout
        android:id="@+id/lntsdk_layout_recharge_success"
       	android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="20dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        >
        <ImageView 
            android:id="@+id/lntsdk_img_recharge_success"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/lntsdk_healthy_hui"
            />
    <TextView 
        android:id="@+id/lntsdk_order_type_text_one"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="充值成功"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:layout_marginLeft="20dp"
        android:gravity="center_horizontal"
        />
    </LinearLayout>
    <ImageView 
        android:layout_width="250dp"
        android:layout_height="1dp"
        android:background="@drawable/lntsdk_params_uplayout_cut_line"
        />
    <LinearLayout
        android:id="@+id/lntsdk_layout_pay_success"
       	android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="20dp"
        >
    <ImageView 
        android:id="@+id/lntsdk_img_pay_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lntsdk_healthy_hui"
        />
    <TextView 
        android:id="@+id/lntsdk_order_type_text_two"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="支付成功"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:layout_marginLeft="20dp"
        android:gravity="center_horizontal"
        />
    </LinearLayout>
    <LinearLayout 
        android:id="@+id/lntsdk_order_type_watch_recharge"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
    <ImageView 
        android:layout_width="250dp"
        android:layout_height="1dp"
        android:background="@drawable/lntsdk_params_uplayout_cut_line"
        />
    <LinearLayout
        android:id="@+id/lntsdk_layout_pay_success_watch"
       	android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="20dp"
        >
    <ImageView 
        android:id="@+id/lntsdk_img_pay_success_watch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lntsdk_healthy_hui"
        />
    <TextView 
        android:id="@+id/lntsdk_order_type_text_three"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="握奇手环"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:layout_marginLeft="20dp"
        android:gravity="center_horizontal"
        />
    </LinearLayout>
    <ImageView 
        android:layout_width="250dp"
        android:layout_height="1dp"
        android:background="@drawable/lntsdk_params_uplayout_cut_line"
        />
    <LinearLayout
        android:id="@+id/lntsdk_layout_pay_success_oma"
       	android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="20dp"
        android:visibility="gone"
        >
    <ImageView 
        android:id="@+id/lntsdk_img_pay_success_oma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lntsdk_healthy_hui"
        />
    <TextView 
        android:id="@+id/lntsdk_order_type_text_four"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="内卡充值"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:layout_marginLeft="20dp"
        android:gravity="center_horizontal"
        />
    </LinearLayout>
    <ImageView 
        android:layout_width="250dp"
        android:layout_height="1dp"
        android:background="@drawable/lntsdk_params_uplayout_cut_line"
        />
    <LinearLayout
        android:id="@+id/lntsdk_layout_pay_success_linklove"
       	android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="20dp"
        android:visibility="visible"
        >
    <ImageView 
        android:id="@+id/lntsdk_img_pay_success_linklove"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lntsdk_healthy_hui"
        />
    <TextView 
        android:id="@+id/lntsdk_order_type_text_five"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="手环充值"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:layout_marginLeft="20dp"
        android:gravity="center_horizontal"
        />
    </LinearLayout>
    </LinearLayout>

</LinearLayout>
